{
  "name": "@gustavjorlov/agent",
  "version": "0.3.0",
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "start": "node --env-file=.env dist/index.js",
    "dev": "tsx src/cli.ts",
    "typecheck": "tsc -p . --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "agent": "dist/src/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "0.62.0",
    "dotenv": "^16.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "@types/node": "^20.14.10",
    "vitest": "^3.2.4"
  }
}
